{
  "scripts": {
    "start": "npm run clean && npm-run-all pages -p watch:*",
    "pages": "webpack",
    "build": "npm run clean && npm run pages && npx @11ty/eleventy --config=eleventy-build.config.js",
    "watch:pages": "webpack --watch",
    "watch:11ty": "eleventy --serve --incremental --watch",
    "watch:11ty-nested-webc-fix": "find src/_includes/components/ -type f -name \"*.webc\" | entr sh -c 'sleep 0.2; touch src/_includes/layouts/base.webc'",
    "clean": "rimraf dist/",
    "rename-project-prefix": "run() { node ./tools/scripts/project-prefix-rename.js $1 $2; }; run"
  },
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@11ty/eleventy-plugin-vite": "^4.0.0",
    "@11ty/eleventy-plugin-webc": "^0.11.1",
    "@fullhuman/postcss-purgecss": "^5.0.0",
    "autoprefixer": "^10.4.14",
    "cssnano": "^6.0.1",
    "esbuild": "^0.18.15",
    "glob": "^10.3.3",
    "mini-css-extract-plugin": "^2.7.6",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.27",
    "postcss-cli": "^10.1.0",
    "postcss-csso": "^6.0.1",
    "postcss-import": "^15.1.0",
    "postcss-loader": "^7.3.3",
    "postcss-media-minmax": "^5.0.0",
    "postcss-preset-env": "^9.0.0",
    "prettier": "^3.0.0",
    "replace-in-file": "^7.0.1",
    "rimraf": "^5.0.1",
    "sass": "^1.64.0",
    "tailwindcss": "^3.3.3",
    "ts-loader": "^9.4.4",
    "typescript": "^5.1.6",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  },
  "name": "11ty-ts-starter",
  "version": "1.0.0",
  "description": "11ty typescript starter",
  "main": "index.js",
  "author": "4lador",
  "license": "MIT",
  "private": false,
  "dependencies": {
    "copy-to-clipboard": "^3.3.3",
    "decap-cms-app": "^3.0.8"
  }
}
